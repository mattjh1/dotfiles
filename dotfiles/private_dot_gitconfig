	name = Mattias Holmgren

[gpg]
  format = ssh

[gpg "ssh"]
  program = "/Applications/1Password.app/Contents/MacOS/op-ssh-sign"

[commit]
  gpgsign = true

[fetch]
  prune = true

[init]
  defaultBranch = main
	
[pull]
	rebase = true
	
[color]
	ui = true

[core]
    pager = delta

[interactive]
    diffFilter = delta --color-only

[delta]
    navigate = true  # use n and N to move between diff sections
    side-by-side = true
    features = nord

[delta "nord"]
    syntax-theme = Nord

[merge]
    conflictstyle = diff3

[diff]
    colorMoved = default
